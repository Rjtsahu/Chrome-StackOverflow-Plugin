{
  "name": "Print this page",
  "description": "Adds a print button to the browser.",
  "version": "1.2",
  "background": {
    "scripts": ["./js/preferences.js","background.js"],
    "persistent": false
  },
  "permissions": [
    "storage",
    "activeTab",
    "https://stackoverflow.com/",
    "https://api.stackexchange.com/"
  ],
  "browser_action": {
      "default_title": "Quick StackOverflow",
      "default_icon": "so-icon.png"
  },
  "manifest_version": 2
}