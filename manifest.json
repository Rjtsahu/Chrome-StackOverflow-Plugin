{
  "name": "Quick StackOverflow",
  "description": "Chrome extension to get most relavent answers for you technical query.",
  "version": "1.1",
  "background": {
    "scripts": ["./js/preferences.js","background.js"],
    "persistent": false
  },
  "icons": {
    "16": "img/qs-16x16-icon.png",
    "48": "img/qs-48x48-icon.png",
    "128": "img/qs-128x128-icon.png"
},
  "permissions": [
    "storage",
    "activeTab",
    "https://stackoverflow.com/",
    "https://api.stackexchange.com/"
  ],
  "browser_action": {
      "default_title": "Quick StackOverflow",
      "default_icon": "img/qs-48x48-icon.png"
  },
  "options_page": "setting_page.html",
  "manifest_version": 2
}